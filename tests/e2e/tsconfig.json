{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "strict": true,
      "skipLibCheck": true,
      "declaration": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "traceResolution": false,
      "listEmittedFiles": false,
      "listFiles": false,
      "pretty": true,
      "composite": true,
      "lib": ["es2017"],
      "sourceMap": true,
      "inlineSources": true,
      "outDir": "lib",
      "rootDir": "src"
    },
    "files": ["./sdk.test.ts", "./utils.ts"],
    "references": [
    ]
}
